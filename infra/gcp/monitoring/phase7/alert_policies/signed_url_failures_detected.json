{
  "displayName": "sg-route-opt - Signed URL failures detected",
  "documentation": {
    "content": "Signed URL generation failures were detected. Export artifact links may fail for users until credentials/permissions are fixed.",
    "mimeType": "text/markdown"
  },
  "userLabels": {
    "service": "__SERVICE_NAME__",
    "phase": "phase7",
    "signal": "signed_url",
    "severity": "critical"
  },
  "conditions": [
    {
      "displayName": "Signed URL failure count > 0 in 5m",
      "conditionThreshold": {
        "filter": "resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"__SERVICE_NAME__\" AND metric.type=\"logging.googleapis.com/user/sg_route_opt_signed_url_failure_count\"",
        "aggregations": [
          {
            "alignmentPeriod": "300s",
            "perSeriesAligner": "ALIGN_SUM",
            "crossSeriesReducer": "REDUCE_SUM",
            "groupByFields": [
              "resource.labels.service_name"
            ]
          }
        ],
        "comparison": "COMPARISON_GT",
        "thresholdValue": 0,
        "duration": "0s",
        "trigger": {
          "count": 1
        }
      }
    }
  ],
  "combiner": "OR",
  "enabled": true,
  "alertStrategy": {
    "autoClose": "3600s"
  }
}
