{
  "displayName": "sg-route-opt - Fallback event rate spike",
  "documentation": {
    "content": "Fallback event rate is elevated (Google ETA fallback, Google resequence fallback, or OneMap heuristic fallback). This can indicate upstream API degradation.",
    "mimeType": "text/markdown"
  },
  "userLabels": {
    "service": "__SERVICE_NAME__",
    "phase": "phase7",
    "signal": "fallback_rate",
    "severity": "warning"
  },
  "conditions": [
    {
      "displayName": "Fallback event rate > 0.05/s for 10m",
      "conditionThreshold": {
        "filter": "resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"__SERVICE_NAME__\" AND metric.type=\"logging.googleapis.com/user/sg_route_opt_fallback_event_count\"",
        "aggregations": [
          {
            "alignmentPeriod": "60s",
            "perSeriesAligner": "ALIGN_RATE",
            "crossSeriesReducer": "REDUCE_SUM",
            "groupByFields": [
              "resource.labels.service_name"
            ]
          }
        ],
        "comparison": "COMPARISON_GT",
        "thresholdValue": 0.05,
        "duration": "600s",
        "trigger": {
          "count": 1
        }
      }
    }
  ],
  "combiner": "OR",
  "enabled": true,
  "alertStrategy": {
    "autoClose": "3600s"
  }
}
